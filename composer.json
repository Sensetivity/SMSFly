{
    "name": "sensetivity/sms-fly-cakephp-3.x",
    "description": "SMSFly plugin for CakePHP 3.x",
    "type": "cakephp-plugin",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Mykola Pukhalskyi",
            "email": "notfound.la2.asterios.tm@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/Sensetivity/SMSFly"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SMSFly\\": "./plugins/SMSFly/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SMSFly\\Test\\": "./plugins/SMSFly/src",
            "Cake\\Test\\": "./plugins/SMSFly/src/tests"
        }
    }
}
